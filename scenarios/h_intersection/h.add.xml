<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">

    <inductionLoop id="eBus" lane="e1_1" pos="440" vTypes="BUS" file="NUL"/>
    <inductionLoop id="wBus" lane="w2_1" pos="440" vTypes="BUS" file="NUL"/>

    <busStop id="e1" lane="e1_0" startPos="400" endPos="420" lines="100"/>
    <busStop id="w2" lane="w2_0" startPos="400" endPos="420" lines="101"/>
    <vType id="BUS" color="0,1,0" vClass="bus"/>

    <flow id="bus_e" type="BUS" from="e1" to="e2" line="100" begin="0" end="3600" period="70" departLane="best" departSpeed="max">
        <stop busStop="e1" duration="20"/>
    </flow>

    <flow id="bus_w" type="BUS" from="w2" to="w1" line="101" begin="0" end="3600" period="70" departLane="best" departSpeed="max">
        <stop busStop="w2" duration="20"/>
    </flow>

    <tlLogic id="h2" type="NEMA" programID="NEMA" offset="0">
        <param key="detector-length" value="20"/>
        <param key="detector-length-leftTurnLane" value="10"/>
        <param key="total-cycle-length" value="150"/>
        <param key="ring1" value="1,2,3,4"/>
        <param key="ring2" value="5,6,7,8"/>
        <param key="barrierPhases" value="2,6"/>
        <param key="coordinate-mode" value="false"/>
        <param key="barrier2Phases" value="4,8"/>
        <param key="minRecall" value="4,8"/>
        <param key="maxRecall" value=""/>
        <param key="whetherOutputState" value="true"/>
        <param key="fixForceOff" value="true"/>
        <param key="show-detectors" value="true" />
        <param key="ignore-errors" value="true"/>

        <condition id="ebus" value="a:eBus > 0"/>
        <condition id="wbus" value="a:wBus > 0"/>
        <param key="show-conditions" value="ebus wbus"/>

        <phase duration="90" state="grrgrrgrGgrr" minDur="5" maxDur="20" vehext="2" yellow="3" red="2" name="3"/>
        <phase duration="90" state="GGrgrrgrrgrr" minDur="5" maxDur="40" vehext="2" yellow="3" red="2" name="4"/>
        <phase duration="90" state="grGgrrgrrgrr" minDur="5" maxDur="20" vehext="2" yellow="3" red="2" name="7"/>
        <phase duration="90" state="grrgrrGGrgrr" minDur="5" maxDur="40" vehext="2" yellow="3" red="2" name="8"/>
        <phase duration="90" state="grrgrGgrrgrr" minDur="5" maxDur="20" vehext="2" yellow="3" red="2" name="1"/>
        <phase duration="90" state="grrgrrgrrGGr" minDur="5" maxDur="50" vehext="2" yellow="3" red="2" name="2" earlyTarget="ebus"/>
        <phase duration="90" state="grrgrrgrrgrG" minDur="5" maxDur="20" vehext="2" yellow="3" red="2" name="5"/>
        <phase duration="90" state="grrGGrgrrgrr" minDur="5" maxDur="50" vehext="2" yellow="3" red="2" name="6" earlyTarget="wbus"/>
    </tlLogic>
</additional>
